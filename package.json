{
  "name": "check-reporter",
  "version": "1.0.0",
  "description": "A Probot app",
  "author": "Bob van der Linden <bobvanderlinden@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com//check-reporter.git",
  "homepage": "https://github.com//check-reporter",
  "bugs": "https://github.com//check-reporter/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --external:pg-native",
    "dev": "nodemon ts-node src/index.ts",
    "start": "node --stack-size=1024 --stack-trace-limit=1000 dist/index.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "pg": "^8.7.1",
    "probot": "^12.3.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.2",
    "@types/pg": "^8.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "esbuild": "^0.12.21",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-typescript": "^0.14.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.2",
    "smee-client": "^1.2.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 14.17.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "root": true,
    "env": {
      "node": true
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
